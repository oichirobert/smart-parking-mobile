{
  "expo": {
    "name": "SmartParking",
    "slug": "smart-parking-mobile",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/app.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.company.smartparking",
      "supportsTablet": true
    },
    "android": {
      "package": "com.company.smartparking",
      "config": {
        "googleMaps": { "apiKey": "" }   
     }
    }
  }
}